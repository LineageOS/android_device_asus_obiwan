# Battery
/batinfo(/.*)?                                            u:object_r:battery_file:s0
/dev/block/platform/soc/1d84000\.ufshc/by-name/batinfo    u:object_r:battery_block_device:s0

# Camera
/vendor/lib(64)?/libipebpsstriping\.so    u:object_r:same_process_hal_file:s0

# Display
/(vendor|system/vendor)/bin/irisConfig    u:object_r:vendor_iris_config_exec:s0

# Fingerprint
/(vendor|system/vendor)/bin/hw/android\.hardware\.biometrics\.fingerprint@2\.3-service\.obiwan    u:object_r:hal_fingerprint_default_exec:s0
/dev/goodix_fp                                                                                    u:object_r:fingerprint_device:s0

# Sensors
/dev/asusLightSensor    u:object_r:sensors_device:s0
/dev/asusProxSensor     u:object_r:sensors_device:s0
/dev/lsensor            u:object_r:sensors_device:s0
/dev/psensor            u:object_r:sensors_device:s0

# Vibrator
/(vendor|system/vendor)/bin/hw/android\.hardware\.vibrator\.service\.obiwan    u:object_r:hal_vibrator_default_exec:s0
